ethernet_rename:
  - new: wan0
    old: eno3
  - new: wan1
    old: eno13
  - new: lan0
    old: ens9
  - new: lan1
    old: ens12
ethernet_config:
  lo:
    bootproto: loopback
    params:
      dns-nameservers: 8.8.8.8 4.4.4.4
      dns-search: maas
  ens10:
    bootproto: dhcp
    params:
      mtu: 1500
