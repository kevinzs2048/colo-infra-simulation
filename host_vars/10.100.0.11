ethernet_rename:
  - new: wan0
    old: eno3
  - new: wan1
    old: eno4
  - new: lan0
    old: ens2
  - new: lan1
    old: ens2d1
ethernet_config:
  lo:
    bootproto: loopback
    params:
      dns-nameservers: 8.8.8.8 4.4.4.4
      dns-search: maas
  eno2:
    bootproto: static
    params:
      address: 10.100.0.11
      netmask: 255.255.0.0
      gateway: 10.100.0.1
      mtu: 1500
  eno1:
    bootproto: dhcp
    params:
      mtu: 1500
